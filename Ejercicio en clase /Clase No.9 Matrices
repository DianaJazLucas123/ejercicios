#include <iostream>
#include"Eigen/Dense"

using namespace std;
using namespace Eigen;

int main()
{
    
    MatrixXd A(3, 3);
    A << 1, 2, 3, 
         4, 5, 6, 
         1, 1, 1;   
    cout << A;
    cout << endl;
    cout << endl;

    MatrixXd B(3, 3);
    B << -1, 0, -1,
        2, 1, 5,
        1, 1, 1;    
    cout << B;
    cout << endl;
    cout << endl;

    MatrixXd C(3, 3);
    C << 0, 0, 0,
        0, 0, 0,
        0, 0, 0;
    cout << C;
    cout << endl;
    cout << endl;

    cout << "C= A+B" << endl;
    C = A + B;
    cout << C;
    cout << endl;
    cout << endl;

    cout << "C= A-B" << endl;
    C = A - B;
    cout << C;
    cout << endl;
    cout << endl;

    cout << "C= A*B" << endl;
    C = A * B;
    cout << C;
    cout << endl;
    cout << endl;

    cout << "Determinante de A" << endl;
    cout << A.determinant();
    cout << endl;
    cout << endl;
}
